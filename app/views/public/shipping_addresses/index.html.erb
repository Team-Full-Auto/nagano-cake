<%= render 'layout/header'%>
<div class="container">
  <div class="row">
    <p>配送先登録一覧</p>
<%= form_with model: @order,  local: true do |f| %>
<%= f.label :postcode,"郵便番号"　%>
<%= f.text_field :postcode %>
<%= f.label :address,"住所"　%>
<%= f.text_field :adress %>
<%= f.lavel :address_name,"宛名"%>
<%= f.text_field :address_name %>
<%= f.submit "新規登録" %>
<% end %>
<table >
  <thead>
    <tbody>
      <% books.each do |book| %>
      <tr>
        <td><%= link_to(shipping_addresses) do %>

        <td><%= link_to shipping_addresses.adress,adress, class: "book_#{book.id}" %></td>
        <td><%= book.body %></td>
      </tr>
    <% end %>
    </tbody>
</table>

</div>
</div>